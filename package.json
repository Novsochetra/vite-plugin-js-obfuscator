{
  "name": "@sochetra-nov/vite-plugin-js-obfuscator",
  "version": "1.0.0",
  "description": "A Vite plugin to obfuscate JavaScript output using javascript-obfuscator.",
  "main": "index.js",
  "devDependencies": {
    "release-it": "^19.0.4"
  },
  "scripts": {
    "release": "release-it"
  },
  "repository": "git+https://github.com/Novsochetra/vite-plugin-js-obfuscator.git",
  "keywords": [
    "vite",
    "vite-plugin",
    "obfuscation",
    "javascript-obfuscator",
    "security"
  ],
  "author": "Sochetra NOV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Novsochetra/vite-plugin-js-obfuscator/issues"
  },
  "homepage": "https://github.com/Novsochetra/vite-plugin-js-obfuscator#readme",
  "peerDependencies": {
    "vite": "^4 || ^5"
  },
  "dependencies": {
    "javascript-obfuscator": "^4.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "release: v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true,
      "token": "${NPM_TOKEN}"
    }
  }
}